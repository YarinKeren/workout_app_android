<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pagewallpaper"
    android:textAlignment="center"
    android:textDirection="firstStrongLtr"
    tools:context=".backActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#28C5C5C5"
        android:fontFamily="@font/workoutfont"
        android:text="@string/back_workout"
        android:textColor="#FFFFFF"
        android:textSize="85sp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/weight"
        android:layout_width="81dp"
        android:layout_height="wrap_content"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:fontFamily="@font/oldsportathletic"
        android:text="@string/weightTV"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.882"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <TextView
        android:id="@+id/exercise"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:fontFamily="@font/oldsportathletic"
        android:text="@string/exerciseTV"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <TextView
        android:id="@+id/repetitions"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:fontFamily="@font/oldsportathletic"
        android:text="@string/repsTV"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <TextView
        android:id="@+id/weightTV"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.879"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weight"
        app:layout_constraintVertical_bias="0.06" />

    <TextView
        android:id="@+id/weightTV2"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weightTV1"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:id="@+id/repsTV"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/repetitions"
        app:layout_constraintVertical_bias="0.06" />

    <EditText
        android:id="@+id/exerciseTV2"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:contextClickable="false"
        android:ems="4"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text="@string/new_exerciseTV"
        android:textAlignment="center"
        android:textDirection="firstStrongLtr"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exerciseTV1"
        app:layout_constraintVertical_bias="0.15"
        tools:ignore="Autofill,LabelFor,TextFields" />

    <EditText
        android:id="@+id/exerciseTV3"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:contextClickable="false"
        android:ems="4"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text="@string/new_exerciseTV"
        android:textAlignment="center"
        android:textDirection="firstStrongLtr"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exerciseTV2"
        app:layout_constraintVertical_bias="0.2"
        tools:ignore="Autofill,LabelFor,TextFields" />

    <TextView
        android:id="@+id/weightTV1"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weightTV"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/weightTV3"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weightTV2"
        app:layout_constraintVertical_bias="0.2" />

    <TextView
        android:id="@+id/repsTV3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/repsTV2"
        app:layout_constraintVertical_bias="0.2" />

    <EditText
        android:id="@+id/exerciseTV1"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:contextClickable="false"
        android:ems="4"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text="@string/new_exerciseTV"
        android:textAlignment="center"
        android:textDirection="firstStrongLtr"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exerciseTV"
        app:layout_constraintVertical_bias="0.1"
        tools:ignore="Autofill,LabelFor,TextFields" />

    <TextView
        android:id="@+id/repsTV1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/repsTV"
        app:layout_constraintVertical_bias="0.1" />

    <EditText
        android:id="@+id/exerciseTV"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:contextClickable="false"
        android:ems="4"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text="@string/new_exerciseTV"
        android:textAlignment="center"
        android:textDirection="firstStrongLtr"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exercise"
        app:layout_constraintVertical_bias="0.06"
        tools:ignore="Autofill,LabelFor,TextFields" />

    <TextView
        android:id="@+id/repsTV2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/roundtimer"
        android:backgroundTint="#C8C5C5C5"
        android:clickable="false"
        android:ems="4"
        android:fontFamily="@font/sairasemi"
        android:gravity="center"
        android:text=""
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongLtr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/repsTV1"
        app:layout_constraintVertical_bias="0.15" />


</androidx.constraintlayout.widget.ConstraintLayout>